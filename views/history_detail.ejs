<%-
include('partials/header.ejs');
%>

<table class="mt-5 w-5/12 mx-auto  bedrijfTable">
    <tr class="flex">
      <td class="font-bold w-1/5 bedrijfHoofdingLabelX" >Naam</td> <td class="w-4/5"> <%= bedrijf.name %></td>
    </tr>
    <tr class="flex"> 
      <td class="font-bold  w-1/5 bedrijfHoofdingLabelX">Adres</td> <td class="w-4/5"><%= bedrijf.address.street %> <%= bedrijf.address.number %></td>
    </tr>
    <tr class="flex">
      <td class=" w-1/5 bedrijfHoofdingLabelX"> </td> <td class="w-4/5"><%= bedrijf.address.postalCode %> <%= bedrijf.address.city %></td>
    </tr>

    <tr class= ""><td class="h-5"></td></tr>

    <tr class="bedrijfRow">
      <td class="font-bold historyLabel ">Datum neerlegging</td> <td class="historyValue text-right"><%= bedrijf.depositDate %></td>
    </tr>
    <tr class="bedrijfRow">
      <td class="font-bold historyLabel">Eigen vermogen</td> <td class="text-right historyValue"><%= bedrijf.eigenVermogen %></td>
    </tr>
    <tr class="bedrijfRow">
      <td class="font-bold historyLabel">Schulden</td> <td class="text-right historyValue"><%= bedrijf.schulden %></td>
    </tr>
    <tr class="bedrijfRow">
      <td class="font-bold historyLabel">Bedrijfswinst</td> <td class="text-right historyValue"><%= bedrijf.bedrijfswinst %></td>
    </tr>
    <tr class="bedrijfRow">
      <td class="font-bold historyLabel">Opzoekdatum:</td> <td class="text-right historyValue"><%= bedrijf.opzoekDatum %></td>
    </tr>
    </table>
    <%-
    include('partials/footer.ejs');
    %>
  </body>
  <script src="assets/history.script.js"></script>
  </html>